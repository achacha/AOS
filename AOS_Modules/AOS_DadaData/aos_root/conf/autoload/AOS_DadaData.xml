<?xml version="1.0" encoding="utf-8"?>
<!-- /config/AOS_DadaData -->
<AOS_DadaData>
	<dataset name="dada">
		<!-- Set specific data -->
		<data type="noun">dada/dataset_dada/noun.dat</data>
		<data type="noun_compound">dada/dataset_dada/noun_compound.dat</data>
		<data type="noun_proper">dada/dataset_dada/noun_proper.dat</data>
		<data type="verb">dada/dataset_dada/verb.dat</data>
		<data type="adjective">dada/dataset_dada/adjective.dat</data>
		<data type="adverb">dada/dataset_dada/adverb.dat</data>
		<data type="location">dada/dataset_dada/location.dat</data>

		<!-- Inherit from common base data -->
		<data type="sketch">dada/base/sketch.dat</data>
		<data type="connect">dada/base/connect.dat</data>
		<data type="preposition">dada/base/preposition.dat</data>
		<data type="pronoun_s">dada/base/pronoun_singular.dat</data>
		<data type="pronoun_p">dada/base/pronoun_plural.dat</data>
		<data type="pronoun_s_pos">dada/base/pronoun_singular_possesive.dat</data>
		<data type="pronoun_p_pos">dada/base/pronoun_plural_possesive.dat</data>
		<data type="pronoun_s_pos_pers">dada/base/pronoun_singular_possesive_personal.dat</data>
		<data type="pronoun_p_pos_pers">dada/base/pronoun_plural_possesive_personal.dat</data>
	</dataset>

	<dataset name="lyrics">
		<!-- Set specific data -->
		<data type="noun">dada/dataset_lyrics/noun.dat</data>
		<data type="noun_compound">dada/dataset_lyrics/noun_compound.dat</data>
		<data type="noun_proper">dada/dataset_lyrics/noun_proper.dat</data>
		<data type="verb">dada/dataset_lyrics/verb.dat</data>
		<data type="adjective">dada/dataset_lyrics/adjective.dat</data>
		<data type="adverb">dada/dataset_lyrics/adverb.dat</data>
		<data type="location">dada/dataset_lyrics/location.dat</data>

		<!-- Inherit from common base data -->
		<data type="sketch">dada/base/sketch.dat</data>
		<data type="connect">dada/base/connect.dat</data>
		<data type="preposition">dada/base/preposition.dat</data>
		<data type="pronoun_s">dada/base/pronoun_singular.dat</data>
		<data type="pronoun_p">dada/base/pronoun_plural.dat</data>
		<data type="pronoun_s_pos">dada/base/pronoun_singular_possesive.dat</data>
		<data type="pronoun_p_pos">dada/base/pronoun_plural_possesive.dat</data>
		<data type="pronoun_s_pos_pers">dada/base/pronoun_singular_possesive_personal.dat</data>
		<data type="pronoun_p_pos_pers">dada/base/pronoun_plural_possesive_personal.dat</data>
	</dataset>

	<dataset name="debug">
		<!-- Debug data contains one word of the type used -->
		<data type="noun">dada/dataset_debug/noun.dat</data>
		<data type="noun_compound">dada/dataset_debug/noun_compound.dat</data>
		<data type="noun_proper">dada/dataset_debug/noun_proper.dat</data>
		<data type="verb">dada/dataset_debug/verb.dat</data>
		<data type="adjective">dada/dataset_debug/adjective.dat</data>
		<data type="adverb">dada/dataset_debug/adverb.dat</data>
		<data type="location">dada/dataset_debug/location.dat</data>
		<data type="sketch">dada/dataset_debug/sketch.dat</data>
		<data type="connect">dada/dataset_debug/connect.dat</data>
		<data type="preposition">dada/dataset_debug/preposition.dat</data>
		<data type="pronoun_s">dada/dataset_debug/pronoun_singular.dat</data>
		<data type="pronoun_p">dada/dataset_debug/pronoun_plural.dat</data>
		<data type="pronoun_s_pos">dada/dataset_debug/pronoun_singular_possesive.dat</data>
		<data type="pronoun_p_pos">dada/dataset_debug/pronoun_plural_possesive.dat</data>
		<data type="pronoun_s_pos_pers">dada/dataset_debug/pronoun_singular_possesive_personal.dat</data>
